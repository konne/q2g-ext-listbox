{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "q2g-listbox": {
      "root": "",
      "sourceRoot": "listbox",
      "projectType": "application",
      "prefix": "q2g-listbox",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/listbox-tmp",
            "index": "listbox/index.html",
            "main": "listbox/main.ts",
            "polyfills": "listbox/polyfills.ts",
            "tsConfig": "listbox/tsconfig.app.json",
            "styles": [
              "listbox/styles.scss"
            ],
            "scripts": [{
              "input": "node_modules/document-register-element/build/document-register-element.js"
            }],
            "customWebpackConfig": {
                "path": "./webpack/config/webpack.config.js"
            },
            "preserveSymlinks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [{
                "replace": "listbox/environments/environment.ts",
                "with": "listbox/environments/environment.prod.ts"
              }],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [{
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              }]
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "sconfig.spec.json",
            "karmaConfig": "s",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "listbox/tsconfig.app.json",
              "listbox/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "q2g-listbox"
}
